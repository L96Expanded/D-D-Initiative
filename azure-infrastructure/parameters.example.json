# Azure Infrastructure Deployment Parameters
# Copy this file to parameters.json and fill in your values

{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment": {
      "value": "prod"
    },
    "location": {
      "value": "eastus"
    },
    "dbAdminPassword": {
      "value": "YOUR_SECURE_PASSWORD_HERE"
    },
    "secretKey": {
      "value": "YOUR_SECRET_KEY_HERE"
    }
  }
}
